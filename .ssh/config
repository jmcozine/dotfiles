
host home
  hostname skrrgh.ddns.net
  port 22321

host socks
  Hostname skrrgh.ddns.net
  Port 22321
  Compression yes
  DynamicForward 9000
  RequestTTY no

host bitbucket.org
  identityfile ~/.ssh/bitbucket

host switch
  KexAlgorithms +diffie-hellman-group-exchange-sha1
  Ciphers +aes128-cbc,aes192-cbc,aes256-cbc

