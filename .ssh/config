CanonicalizeHostname yes
CanonicalDomains yuxian.network

host home
  hostname skrrgh.ddns.net
  port 22321

host socks
  Hostname skrrgh.ddns.net
  Port 22321
  Compression yes
  DynamicForward 9000
  RequestTTY no

host bitbucket.org
  identityfile ~/.ssh/bitbucket

host switch
  Hostname %h.yuxian.network
  KexAlgorithms +diffie-hellman-group-exchange-sha1
  Ciphers +aes128-cbc,aes192-cbc,aes256-cbc
  RequestTTY no
  RemoteCommand none

Match host "gitlab.yuxian.network" user "git"
  RemoteCommand none

Host vcsa unifi gw
  Hostname %h.yuxian.network
  RemoteCommand none

Host *.yuxian.network
  RequestTTY yes
  RemoteCommand tmux -u new -A -s ssh

# vim:ts=2:sw=2
